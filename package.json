{
  "name": "pew",
  "version": "1.0.0",
  "description": "pew",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js --progress --colors --config webpack.build.config.js",
    "setup": "npm install && git submodule init && npm run updateExamples && cd examples && npm install && cd ..",
    "updateExamples": "git submodule update",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run examples",
    "examples": "./node_modules/webpack/bin/webpack.js --progress --colors --watch --config webpack.examples.config.js",
    "update": "git submodule update --remote",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-regenerator": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "devtron": "^1.1.2",
    "eslint": "^3.13.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "flow-bin": "^0.39.0",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apophanystudios/pew.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apophanystudios/pew/issues"
  },
  "homepage": "https://github.com/apophanystudios/pew#readme",
  "dependencies": {
    "brfs": "*",
    "matter-collision-events": "^0.1.0",
    "matter-js": "^0.11.1",
    "pixi.js": "^4.2.3"
  }
}
